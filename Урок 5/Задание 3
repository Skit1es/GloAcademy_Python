num = int(input('Введите шестизначное число: '))
a = (num // 1000) // 100
b = ((num // 1000) // 10) % 10
c = (num // 1000) % 10
x = (num % 1000) // 100
y = (num % 1000) // 10 % 10
z = (num % 1000) % 10
if (a + b + c) == (x + y + z):
    print(f'Билет {num} счастливый')
else:
    print(f'Билет {num} НЕсчастливый')
